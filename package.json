{
  "name": "@haibun/sarif",
  "version": "1.8.1",
  "description": "sarif index for haibun",
  "main": "build/sarif",
  "files": [
    "build/"
  ],
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint -c .eslintrc.json --ext .ts,.js src",
    "test": "jest --config jest.config.ts",
    "test-haibun": "build/cli.js test/projects/web",
    "coverage": "jest --config jest.config.ts --coverage",
    "test-watch": "jest --watch",
    "build": "tsc -b .",
    "tsc-watch": "tsc --watch",
    "tsc": "tsc",
    "prepare": "npm run build",
    "prepublishOnly": "tsc -b .",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@haibun/core": "^1.5.0",
    "@haibun/domain-storage": "^1.6.0",
    "@haibun/out-review": "^1.6.0",
    "@types/sarif": "^2.1.4"
  }
}